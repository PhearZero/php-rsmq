{
  "name": "andrewbreksa/rsmq",
  "description": "Redis Simple Message Queue.",
  "type": "library",
  "license": "MIT",
  "version": "0.2.1",
  "authors": [
    {
      "name": "emre can islambey",
      "email": "eislambey@gmail.com"
    },
    {
      "name": "Andrew Breksa",
      "email": "andrew@andrewbreksa.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-mbstring": "*",
    "predis/predis": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "AndrewBreksa\\RSMQ\\": "src/"
    }
  },
  "suggest": {
    "ext-phpiredis": "(Allows faster serialization and deserialization of the Redis protocol)"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3 || ^9.0",
    "phpstan/phpstan": "^0.11.12 || ^0.12.0",
    "roave/security-advisories": "dev-master",
    "vimeo/psalm": "^3.12",
    "squizlabs/php_codesniffer": "^3.5",
    "sensiolabs/security-checker": "^6.0"
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "scripts": {
    "test": "phpunit",
    "phpstan": "phpstan analyse -c phpstan.neon",
    "cbf": "php ./vendor/bin/phpcbf tests src",
    "psalm": "php ./vendor/bin/psalm --show-info=true",
    "security": "php ./vendor/bin/security-checker security:check ./composer.lock"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
